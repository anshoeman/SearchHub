    <style>
      #mySidenav a {
        position: absolute;
        left: -80px;
        transition: 0.3s;
        padding: 15px;
        width: 100px;
        text-decoration: none;
        font-size: 20px;
        color: navy;
        border-radius: 0 5px 5px 0;
      }

      #mySidenav a:hover {
        left: 0;
      }

      #home {
        top: 20px;
        background-color: #e4e2c8;
      }

      #course {
        top: 80px;
        background-color: #94c8f1;
      }

      #myprofile {
        top: 140px;
        background-color: #e4e2c8;
      }

      #contact {
        top: 200px;
        background-color: #94c8f1;
      }
      body {
        font-family: "Poppins", sans-serif;
      }
    </style>
  </head>
